#
# The IceGrid locator proxy.
#
Ice.Default.Locator=TestGrid/Locator:default -p 4061

#
Ice.Admin.Endpoints=tcp -h 127.0.0.1
Ice.ProgramName=SimulatedAxisServer

#
# The adapter info
#
SimulatedAxisAdapter.Endpoints=tcp -p 10000
SimulatedAxisAdapter.AdapterId=SimulatedAxisAdapter

#SimulatedAxis=SimulatedAxis@SimulatedAxisAdapter

#
# Server descriptor properties
#
Identity=SimulatedAxis

#
# Increase the ThreadPool size
#
Ice.ThreadPool.Server.Size=4

#
# Warn about connection exceptions
#
Ice.Warn.Connections=1

#
# Network Tracing
#
# 0 = no network tracing
# 1 = trace connection establishment and closure
# 2 = like 1, but more detailed
# 3 = like 2, but also trace data transfer
#
Ice.Trace.Network=1

#
# Protocol Tracing
#
# 0 = no protocol tracing
# 1 = trace protocol messages
#
#Ice.Trace.Protocol=1

#
# Security Tracing
#
# 0 = no security tracing
# 1 = trace messages
#
#IceSSL.Trace.Security=1

